{
    "name": "victorwitkamp/portalcms",
    "description": "A PHP framework/CMS that is currently in development.",
    "require": {
        "php": ">=7.2.0",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "phpmailer/phpmailer": "~6.0",
        "facebook/graph-sdk": "^5.7",
        "tecnickcom/tcpdf": "6.3.2",
        "ext-fileinfo": "*",
        "symfony/console": "^4.3",
        "sentry/sdk": "2.0.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
